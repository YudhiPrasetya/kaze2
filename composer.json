{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "bensampo/laravel-enum": "^4.2",
        "doctrine/dbal": "^3.0",
        "dompdf/dompdf": "^1.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "geoip2/geoip2": "^2.11",
        "guzzlehttp/guzzle": "^7.0.1",
        "itsgoingd/clockwork": "^5.0",
        "jmrashed/zkteco": "^1.2",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^2.2",
        "laravel/passport": "^10.1",
        "laravel/sanctum": "^2.9",
        "laravel/scout": "^8.6",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/socialite": "^5.2",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.0",
        "mpdf/mpdf": "^8.0",
        "o2system/spl": "^1.0",
        "owen-it/laravel-auditing": "^12.0",
        "phpoffice/phpspreadsheet": "^1.17",
        "spatie/laravel-model-status": "^1.10",
        "spatie/laravel-ray": "^1.17",
        "symfony/yaml": "^5.2",
        "tecnickcom/tcpdf": "^6.4",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.13",
        "laravel/envoy": "^2.6",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Helpers\\Autoload\\": "app/Helpers/Autoload",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Libraries/_/bootstrap.php",
            "app/Helpers/helpers.php",
            "app/Helpers/permission.php",
            "app/Helpers/form.php",
            "app/Helpers/html.php",
            "app/Helpers/scout-driver.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan telescope:publish --ansi"
        ]
    }
}
